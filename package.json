    "css-loader": "^3.1.0",
    "cssnano": "^4.1.10",
    "file-loader": "^4.1.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "sass": "^1.22.7",
    "sass-loader": "^7.1.0",
    "style-loader": "^1.1.3",
